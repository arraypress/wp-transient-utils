{
  "name": "arraypress/wp-transient-utils",
  "version": "1.0.0",
  "description": "A lean WordPress library for working with transients and caching",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-transients-utils/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\TransientUtils\\": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-transient-utils/issues",
    "source": "https://github.com/arraypress/wp-transient-utils"
  },
  "keywords": [
    "wordpress",
    "transients",
    "caching",
    "wp",
    "arraypress",
    "performance",
    "cache-management"
  ]
}